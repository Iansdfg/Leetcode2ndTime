class Solution(object):
    def intersection(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        nums1, nums2 = set(nums1),set(nums2) 
        res = []
        for ele in nums1:
            if ele in nums2:
                res.append(ele)
        return res
        
